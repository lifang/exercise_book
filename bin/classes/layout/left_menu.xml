<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginTop="106px" >

    <LinearLayout
        android:id="@+id/allLinear"
        android:layout_width="206.09px"
        android:layout_height="fill_parent"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/linear"
            android:layout_width="206.09px"
            android:layout_height="1718px"
            android:background="#3f4853"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/ll1"
                android:layout_width="206.09px"
                android:layout_height="141px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/leftmenu_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="68px"
                    android:layout_marginTop="33.94px"
                    android:src="@drawable/leftmenu_11" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="问答"
                    android:textSize="21.38px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll2"
                android:layout_width="206.09px"
                android:layout_height="204px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:id="@+id/leftmenu_12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="68px"
                        android:layout_marginTop="95px"
                        android:src="@drawable/leftmenu_12" />

                    <ImageView
                        android:id="@+id/leftmenu_12red"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20px"
                        android:layout_marginTop="95px"
                        android:layout_toRightOf="@id/leftmenu_12"
                        android:visibility="gone"
                        android:src="@drawable/smallred" />
                </RelativeLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="作业"
                    android:textSize="21.38px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll3"
                android:layout_width="206.09px"
                android:layout_height="204px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:id="@+id/leftmenu_13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="68px"
                        android:layout_marginTop="95px"
                        android:src="@drawable/leftmenu_13" />

                    <ImageView
                        android:id="@+id/leftmenu_13red"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20px"
                        android:layout_marginTop="95px"
                        android:layout_toRightOf="@id/leftmenu_13"
                        android:visibility="gone"
                        android:src="@drawable/smallred" />
                </RelativeLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="通知"
                    android:textSize="21.38px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll4"
                android:layout_width="206.09px"
                android:layout_height="204px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/leftmenu_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="68px"
                    android:layout_marginTop="95px"
                    android:src="@drawable/leftmenu_14" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="卡包"
                    android:textSize="21.38px" />
            </LinearLayout>

            <ImageView
                android:id="@+id/classMenu"
                android:layout_width="155px"
                android:layout_height="79px"
                android:layout_marginLeft="60px"
                android:layout_marginTop="223px"
                android:adjustViewBounds="false"
                android:scaleType="fitStart"
                android:src="@drawable/menuclass" />
            
            
            <LinearLayout
                android:id="@+id/ll6"
                android:layout_width="206.09px"
                android:layout_height="204px"
                android:background="#3f4853"
                android:layout_marginTop="295px"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/leftmenu_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="68px"
                    android:layout_marginTop="95px"
                    android:src="@drawable/leftmenu_quit" />

            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/menuclassll"
            android:layout_width="339.94px"
            android:layout_height="fill_parent"
            android:background="#363e47"
            android:visibility="gone" >

            <ImageView
                android:id="@+id/classback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="19px"
                android:layout_marginTop="21.06px"
                android:src="@drawable/menuclassback" />

            <TextView
                android:id="@+id/teachTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/classback"
                android:layout_marginLeft="32.94px"
                android:layout_marginTop="44.48px"
                android:text="我的班主任" />

            <LinearLayout
                android:id="@+id/teachll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/teachTv"
                android:gravity="center_horizontal" >

                <com.comdosoft.ExerciseBook.tools.CircularImage
                    android:id="@+id/teacherIm"
                    android:layout_width="60px"
                    android:layout_height="60px"
                    android:layout_marginLeft="32px"
                    android:layout_marginTop="39.39px"
                    android:src="@drawable/moren" />

                <TextView
                    android:id="@+id/teachname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.41px"
                    android:layout_marginTop="48px"
                    android:layout_toRightOf="@id/teacherIm"
                    android:text="李达达" />
            </LinearLayout>

            <TextView
                android:id="@+id/classstu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/teachll"
                android:layout_marginLeft="32.94px"
                android:layout_marginTop="40px"
                android:text="我的同学" />

            <GridView
                android:id="@+id/classstugv"
                android:layout_width="339.94px"
                android:layout_height="match_parent"
                android:layout_below="@id/classstu"
                android:layout_marginTop="0px"
                android:listSelector="@null" >
            </GridView>
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>