<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginTop="71.02px" >

    <LinearLayout
        android:id="@+id/allLinear"
        android:layout_width="138.08px"
        android:layout_height="fill_parent"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/linear"
            android:layout_width="138.08px"
            android:layout_height="1151.06px"
            android:background="#3f4853"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/left_menu_ll1"
                android:layout_width="138.08px"
                android:layout_height="100px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:id="@+id/leftmenu_12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="45.56px"
                        android:layout_marginTop="22.73px"
                        android:src="@drawable/leftmenu_12" />

                    <ImageView
                        android:id="@+id/leftmenu_12red"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13.4px"
                        android:layout_marginTop="22.73px"
                        android:layout_toRightOf="@id/leftmenu_12"
                        android:src="@drawable/smallred"
                        android:visibility="gone" />
                </RelativeLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="作业"
                    android:textSize="14.96px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/left_menu_ll2"
                android:layout_width="138.08px"
                android:layout_height="136.68px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/leftmenu_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="45.56px"
                    android:layout_marginTop="63.65px"
                    android:src="@drawable/leftmenu_wenda" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="问答"
                    android:textSize="14.96px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/left_menu_ll3"
                android:layout_width="138.08px"
                android:layout_height="136.68px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:id="@+id/leftmenu_13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="45.56px"
                        android:layout_marginTop="63.65px"
                        android:src="@drawable/leftmenu_13" />

                    <ImageView
                        android:id="@+id/leftmenu_13red"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13.4px"
                        android:layout_marginTop="63.65px"
                        android:layout_toRightOf="@id/leftmenu_13"
                        android:src="@drawable/smallred"
                        android:visibility="gone" />
                </RelativeLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="通知"
                    android:textSize="14.32px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/left_menu_ll4"
                android:layout_width="138.08px"
                android:layout_height="136.68px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/leftmenu_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="45.56px"
                    android:layout_marginTop="63.65px"
                    android:src="@drawable/leftmenu_14" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="卡包"
                    android:textSize="14.32px" />
            </LinearLayout>

            <ImageView
                android:id="@+id/classMenu"
                android:layout_width="103.85px"
                android:layout_height="52.93px"
                android:layout_marginLeft="40.2px"
                android:layout_marginTop="156.11px"
                android:adjustViewBounds="false"
                android:scaleType="fitStart"
                android:src="@drawable/menuclass" />

            <LinearLayout
                android:id="@+id/ll6"
                android:layout_width="138.08px"
                android:layout_height="136.68px"
                android:layout_marginTop="197.65px"
                android:background="#3f4853"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/leftmenu_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="45.56px"
                    android:layout_marginTop="63.65px"
                    android:src="@drawable/leftmenu_quit" />
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/menuclassll"
            android:layout_width="282.75px"
            android:layout_height="fill_parent"
            android:background="#363e47"
            android:visibility="gone" >

            <ImageView
                android:id="@+id/classback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="12.73px"
                android:layout_marginTop="14.11px"
                android:src="@drawable/menuclassback" />

            <TextView
                android:id="@+id/teachTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/classback"
                android:layout_marginLeft="22.39px"
                android:layout_marginTop="29.80px"
                android:text="我的班主任"
                android:textColor="@color/selecttextview" />

            <LinearLayout
                android:id="@+id/teachll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/teachTv"
                android:gravity="center_horizontal" >

                <com.comdosoft.ExerciseBook.tools.CircularImage
                    android:id="@+id/teacherIm"
                    android:layout_width="40.2px"
                    android:layout_height="40.2px"
                    android:layout_marginLeft="21.44px"
                    android:layout_marginTop="26.399px"
                    android:src="@drawable/moren" />

                <TextView
                    android:id="@+id/teachname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10.32px"
                    android:layout_marginTop="32.16px"
                    android:layout_toRightOf="@id/teacherIm"
                    android:text="李达达"
                    android:textColor="@color/white" />
            </LinearLayout>

            <TextView
                android:id="@+id/classstu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/teachll"
                android:layout_marginLeft="22.06px"
                android:layout_marginTop="26.8px"
                android:text="我的同学"
                android:textColor="@color/selecttextview" />

            <GridView
                android:id="@+id/classstugv"
                android:layout_width="282.75px"
                android:layout_height="880px"
                android:layout_below="@id/classstu"
                android:layout_marginTop="0px"
                android:listSelector="@null"
                android:scrollbars="none" >
            </GridView>
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>